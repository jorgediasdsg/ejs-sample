<!DOCTYPE html>
    <%- include('../partials/head');%>
    <body class="container">
        <%- include('../partials/header');%>
        <main>
            <div class="jumbotron">
                <h1>Node com EJS</h1>
                <p>
                    <%= subtitle%>
                </p>
            </div>
            <ul>
                <% messages.forEach(function(message){ %>
                    <li>
                        <strong><%= message.title %></strong><%= message.message %>
                    </li>
                <% }) %>
            </ul>
        </main>
    </body>
    <%- include('../partials/footer');%>
</html>